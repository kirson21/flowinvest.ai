{
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "https://flowinvestaiapp-kirsons-projects.vercel.app/:path*"
    },
    {
      "source": "/app",
      "destination": "https://flowinvestaiapp-kirsons-projects.vercel.app/"
    }
  ],
  "headers": [
    {
      "source": "/app/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}