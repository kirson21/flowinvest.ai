{
  "rewrites": [
    {
      "source": "/login",
      "destination": "https://app.f01i.ai/login"
    },
    {
      "source": "/login/callback",
      "destination": "https://app.f01i.ai/login/callback"
    },
    {
      "source": "/app/:path*",
      "destination": "https://app.f01i.ai/app/:path*"
    },
    {
      "source": "/app",
      "destination": "https://app.f01i.ai/app"
    },
    {
      "source": "/api/:path*",
      "destination": "https://app.f01i.ai/api/:path*"
    }
  ]
}